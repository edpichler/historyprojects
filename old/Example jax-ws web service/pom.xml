						</goals>
						<phase>compile</phase>
					</execution>
				</executions>
				<configuration>
					<sei>com.pichler.ws.WebServiceMaligno</sei>
					<verbose>true</verbose>
					<keep>true</keep>
					<wsdlUrls>
						<wsdlUrl>
							http://api.google.com/GoogleSearch.wsdl
						</wsdlUrl>

					</wsdlUrls>
				</configuration>
